{
  "version": "0.0.0",
  "name": "resolve.imports",
  "repository": "okikio/resolve.imports",
  "description": "A tiny (737b), correct, general-purpose, and configurable subpath \"imports\" resolver without file-system reliance (forked from @lukeed's resolve.exports)",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "bundt",
    "test": "uvu -r esm test"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "esm",
    "subpath",
    "imports",
    "esmodules",
    "fields",
    "modules",
    "resolution",
    "resolve"
  ],
  "devDependencies": {
    "bundt": "1.1.2",
    "esm": "3.2.25",
    "uvu": "0.5.1"
  }
}
